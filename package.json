  "license": "MIT",
  "private": true,
  "description": "Demo of angular & rxdb",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint && yarn lint:styles",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "postinstall": "ng run demo-angular-material-rxdb:version",
    "prepare": "husky",
    "i18n:extract-regex": "ngx-i18n-extract-regex-cli extract --library ngx-translate",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _ --format-indentation '  ' --string-as-default-value '███'",
    "fix-vulnerabilities": "npx yarn-audit-fix && npx browserslist --update-db"
  },
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "^20.0.1",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.0.1",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@gilsdav/ngx-translate-router": "^7.2.1",
    "@gilsdav/ngx-translate-router-http-loader": "^2.0.0",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "lodash-es": "^4.17.21",
    "ngx-app-version": "^2.0.1",
    "ngx-fixed-footer": "^3.2.1",
    "ngx-translate-version": "^4.0.2",
    "rxdb": "^15.39.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "uuid": "^11.1.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-angular": "^19.8.1",
    "@types/jasmine": "^5.1.8",
    "@types/lodash-es": "^4.17.12",
    "@types/uuid": "^10.0.0",
    "@vendure/ngx-translate-extract": "^9.4.0",
    "angular-eslint": "19.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.27.0",
    "husky": "^9.1.7",
    "jasmine-core": "^5.7.1",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lint-staged": "^16.1.0",
    "ngx-devkit-builders": "^1.3.3",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  },
  "engines": {
    "node": ">=24 <26",
    "npm": "please-use-yarn"
  }
