  "private": true,
  "description": "Demo of angular & rxdb",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint && yarn lint:styles",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "postinstall": "ng run demo-angular-material-rxdb:version",
    "prepare": "husky",
    "i18n:extract-regex": "ngx-i18n-extract-regex-cli extract --library ngx-translate",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _ --format-indentation '  ' --string-as-default-value '███'",
    "fix-vulnerabilities": "npx yarn-audit-fix && npx browserslist --update-db"
  },
  "dependencies": {
    "@angular/animations": "^19.1.6",
    "@angular/cdk": "^19.1.4",
    "@angular/common": "^19.1.6",
    "@angular/compiler": "^19.1.6",
    "@angular/core": "^19.1.6",
    "@angular/forms": "^19.1.6",
    "@angular/material": "^19.1.4",
    "@angular/platform-browser": "^19.1.6",
    "@angular/platform-browser-dynamic": "^19.1.6",
    "@angular/router": "^19.1.6",
    "@gilsdav/ngx-translate-router": "^7.2.1",
    "@gilsdav/ngx-translate-router-http-loader": "^2.0.0",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "lodash-es": "^4.17.21",
    "ngx-app-version": "^2.0.0",
    "ngx-fixed-footer": "^3.2.0",
    "ngx-translate-version": "^4.0.0",
    "rxdb": "^15.39.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "uuid": "^11.0.5",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.7",
    "@angular/cli": "^19.1.7",
    "@angular/compiler-cli": "^19.1.6",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-angular": "^19.7.1",
    "@types/jasmine": "^5.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/uuid": "^10.0.0",
    "@vendure/ngx-translate-extract": "^9.4.0",
    "angular-eslint": "19.1.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.1",
    "husky": "^9.1.7",
    "jasmine-core": "^5.6.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lint-staged": "^15.4.3",
    "ngx-devkit-builders": "^1.3.3",
    "postcss": "^8.5.2",
    "prettier": "^3.5.1",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  },
  "engines": {
    "node": ">=22 <24",
    "npm": "please-use-yarn"
