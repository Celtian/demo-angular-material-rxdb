  "license": "MIT",
  "private": true,
  "description": "Demo of angular & rxdb",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint && yarn lint:styles",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "preinstall": "npx only-allow yarn",
    "postinstall": "ng run demo-angular-material-rxdb:version",
    "prepare": "husky install",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _ --format-indentation '  ' --string-as-default-value '███'",
    "fix-vulnerabilities": "npx yarn-audit-fix && npx browserslist --update-db",
    "script": "cd build-scripts && npx ts-node -P tsconfig.ts-node.json --",
    "script:i18n-extract": "npm run script i18n-extract"
  },
  "dependencies": {
    "@angular/animations": "^17.0.3",
    "@angular/cdk": "^17.0.1",
    "@angular/common": "^17.0.3",
    "@angular/compiler": "^17.0.3",
    "@angular/core": "^17.0.3",
    "@angular/forms": "^17.0.3",
    "@angular/material": "^17.0.1",
    "@angular/platform-browser": "^17.0.3",
    "@angular/platform-browser-dynamic": "^17.0.3",
    "@angular/router": "^17.0.3",
    "@gilsdav/ngx-translate-router": "^6.1.0",
    "@gilsdav/ngx-translate-router-http-loader": "^2.0.0",
    "@ngneat/tailwind": "^7.0.3",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "lodash-es": "^4.17.21",
    "ngx-app-version": "^0.1.0",
    "ngx-translate-version": "^1.1.0",
    "rxdb": "^14.17.1",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.1",
    "@angular-eslint/builder": "^17.1.0",
    "@angular-eslint/eslint-plugin": "^17.1.0",
    "@angular-eslint/eslint-plugin-template": "^17.1.0",
    "@angular-eslint/schematics": "^17.1.0",
    "@angular-eslint/template-parser": "^17.1.0",
    "@angular/cli": "^17.0.1",
    "@angular/compiler-cli": "^17.0.3",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@commitlint/cli": "^18.4.2",
    "@commitlint/config-conventional": "^18.4.2",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/jasmine": "^5.1.2",
    "@types/lodash-es": "^4.17.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vendure/ngx-translate-extract": "^8.2.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "jasmine-core": "^5.1.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lint-staged": "^15.1.0",
